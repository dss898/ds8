def binary_search(key,arr):
    low = 0
    high = len(arr)-1
    
    while low <=high:
        mid = (low+high)//2
        
        if arr[mid]==key:
            return mid #found 
        elif arr[mid]<key:
            low=mid+1
        else:
            high=mid-1
            
    return -1 #not found
#fixed sorted list
arr = [10,20,30,40,50]
key = int(input("enter the key value to be searched"))
index = binary_search(key,arr)

if index == -1:
    print("element not found")

else:
    print("element found at index",index)
